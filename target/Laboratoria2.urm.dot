    Seller [ label = "{Seller | + getSellerAdress() : String\l+ getSellerID() : int\l+ getSellerNIP() : double\l+ getSellerName() : String\l+ getSellerPostalCode() : String\l+ getSellerTown() : String\l}" ] 
